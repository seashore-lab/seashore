{
  "name": "@seashore/llm",
  "version": "0.1.0",
  "type": "module",
  "description": "LLM adapters and multimodal support for Seashore Agent Framework",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./adapters": {
      "import": "./dist/adapters.js",
      "types": "./dist/adapters.d.ts"
    },
    "./embedding": {
      "import": "./dist/embedding.js",
      "types": "./dist/embedding.d.ts"
    },
    "./multimodal": {
      "import": "./dist/multimodal.js",
      "types": "./dist/multimodal.d.ts"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "llm",
    "openai",
    "anthropic",
    "gemini",
    "ai",
    "seashore"
  ],
  "license": "MIT",
  "dependencies": {
    "@tanstack/ai": "^0.1.0",
    "@tanstack/ai-openai": "^0.1.0",
    "@tanstack/ai-anthropic": "^0.1.0",
    "@tanstack/ai-gemini": "^0.1.0"
  },
  "peerDependencies": {
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "zod": "^4.0.5"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false
}