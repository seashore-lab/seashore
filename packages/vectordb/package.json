{
  "name": "@seashore/vectordb",
  "version": "0.1.0",
  "type": "module",
  "description": "Vector database with pgvector for Seashore Agent Framework",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./schema": {
      "import": "./dist/schema/index.js",
      "types": "./dist/schema/index.d.ts"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "keywords": [
    "vectordb",
    "pgvector",
    "embedding",
    "similarity-search",
    "seashore"
  ],
  "author": "Seashore Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/seashore-ai/seashore.git",
    "directory": "packages/vectordb"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@seashore/storage": "workspace:*",
    "drizzle-orm": "^0.44.2",
    "pgvector": "^0.2.0"
  },
  "peerDependencies": {
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "drizzle-kit": "^0.31.1",
    "zod": "^4.0.5"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false
}
