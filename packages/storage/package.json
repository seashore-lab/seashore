{
  "name": "@seashorelab/storage",
  "version": "0.1.0",
  "type": "module",
  "description": "PostgreSQL storage layer with Drizzle ORM for Seashore Agent Framework",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./schema": {
      "import": "./dist/schema/index.js",
      "types": "./dist/schema/index.d.ts"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "keywords": [
    "storage",
    "postgresql",
    "drizzle",
    "orm",
    "seashore"
  ],
  "author": "Seashore Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/seashore-ai/seashore.git",
    "directory": "packages/storage"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "drizzle-orm": "^0.44.2",
    "postgres": "^3.4.7"
  },
  "devDependencies": {
    "drizzle-kit": "^0.31.1"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false
}